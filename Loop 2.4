#include<iostream>

using namespace std;

int main() {
    int n;
    cin >> n;
    for (int i = 2; i < n-1; i++) {
        if ((n-1) % i == 0) {
            cout << "No";
            exit(0);
        }
    }
    for (int i = 2; i < n + 1; i++) {
        if ((n+1) % i == 0) {
            cout << "No";
            exit(0);
        }
    }
    cout << "Yes";
}
